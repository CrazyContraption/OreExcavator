					Controls: "{$Mods.OreExcavator.Config.Client.Headers.Header}{$Mods.OreExcavator.Config.Common.HeaderBuffer} Управление"
				}

				ShowWelcome: {
					Label: "[i/p0:4792]{$Mods.OreExcavator.Config.Common.IconBuffer} Показать сообщение при запуске"
					Description:
						'''
						Когда отключено, приветственные сообщения будут скрыты для этой версии мода.

						                  Новые версии будут снова включать эту функцию.
						'''
				}

				ShowCursorTooltips: {
					Label: "[i/p0:5075]{$Mods.OreExcavator.Config.Common.IconBuffer} Показать всплывающую подсказку для добычи"
					Description:
						'''
						Когда отключено, удерживание клавиши добычи больше не будет отображать контекстную подсказку.
						Пожалуйста, включите это перед сообщением об ошибках!
						'''
				}

				ShowItemTooltips: {
					Label: "[i/p0:267]{$Mods.OreExcavator.Config.Common.IconBuffer} Показывать всплывающие подсказки для предметов"
					Description:
						'''
						Когда отключено, предметы, стены и плитки больше не будут отображать контекстную подсказку.
						Пожалуйста, включите это перед сообщением об ошибках
						'''
				}

				ReducedEffects: {
					Label: "[i/p0:150]{$Mods.OreExcavator.Config.Common.IconBuffer} Сокращенные эффекты"
					Description:
						'''
						Имеется задержка? При отключении мод попытается работать в сокращенном режиме, обеспечивая лучшую графическую производительность.
						Функционально это ничего не изменит, за исключением косметических различий.
						'''
				}

				RefillMana: {
					Label: "[i/p0:555]{$Mods.OreExcavator.Config.Common.IconBuffer} Автоматическое использование зелий маны для добычи"
					Description: При включении и включенных требованиях к мане в мире, мод должен попытаться пополнить ваш запас маны, если вы его исчерпаете. {$Mods.OreExcavator.Config.Common.Performance.NoImpact}.
				}

				DoDebugStuff: {
					Label: "[i/p0:37]{$Mods.OreExcavator.Config.Common.IconBuffer} Показать журналы отладки"
					Description:
						'''
						Когда отключено, отладочные журналы будут скрыты. Включите это, если у вас возникают проблемы с пониманием мода.
						      		    Пожалуйста, включите это перед сообщением об ошибках!
						'''
				}

				RecursionLimit: {
					Label: '''[i/p0:18]{$Mods.OreExcavator.Config.Common.IconBuffer} 	Предел модификации блоков'''
					Description: Определяет максимальное количество плиток, которые можно изменить за одну добычу. Большие значения негативно скажутся на производительности!
				}

				DoDiagonals: {
					Label: "[i/p0:2799]{$Mods.OreExcavator.Config.Common.IconBuffer} Включить диагональный поиск"
					Description: При включении алгоритм добычи также будет проверять совпадения напрямую по диагонали от себя. Отключение этого УЛУЧШИТ производительность!
				}

				RecursionDelay: {
					Label: "[i/p0:3099]{$Mods.OreExcavator.Config.Common.IconBuffer} Задержка при разрушении блоков"
					Description:
						'''
						Задержка в миллисекундах между разрушениями блоков. 
						Большие значения могут улучшить производительность!
						'''
				}

				InititalChecks: {
					Label: "[i/p0:321]{$Mods.OreExcavator.Config.Common.IconBuffer} Выполнить начальные проверки белого списка"
					Description:
						'''
						При включении алгоритм проверяет белые и черные списки ПЕРЕД попыткой добычи, когда связан с правой кнопкой мыши. 
						Это имеет небольшое влияние на производительность, но может обеспечить более стабильное поведение! {$Mods.OreExcavator.Config.Common.Performance.SmallImpact}
						   
						'''
				}

				TileWhitelistAll: {
					Label: "[i/p0:3509]{$Mods.OreExcavator.Config.Common.IconBuffer} Добавить все плитки в белый список"
					Description: При включении все плитки будут добавлены в белый список по умолчанию. {$Mods.OreExcavator.Config.Common.Performance.EnableImproves}!
				}

				TileWhitelist: {
					Label: "[i/p0:2695]{$Mods.OreExcavator.Config.Common.IconBuffer} Белый список плиток"
					Description:
						'''
						Настройте этот список, чтобы вручную указать, какие плитки могут быть добыты цепной добычей.
						Префикс определяет мод которому они принадлежат. Также подчиняется черному списку хоста.
						{$Mods.OreExcavator.Config.Common.Performance.NoImpact}.
						'''
				}

				WallWhitelistAll: {
					Label: "[i/p0:196]{$Mods.OreExcavator.Config.Common.IconBuffer} Добавить все стены в белый список"
					Description: При включении все стены будут добавлены в белый список по умолчанию. {$Mods.OreExcavator.Config.Common.Performance.EnableImproves}!
				}

				WallWhitelist: {
					Label: "[i/p0:2696]{$Mods.OreExcavator.Config.Common.IconBuffer} Белый список стен"
					Description: Настройте этот список, чтобы вручную указать, какие стены могут быть добыты цепной добычей. Префикс определяет мод, которому они принадлежат. Также подчиняется черному списку хоста {$Mods.OreExcavator.Config.Common.Performance.NoImpact}.
				}

				ItemWhitelistAll: {
					Label: "[i/p0:9]{$Mods.OreExcavator.Config.Common.IconBuffer} Добавить все предметы в белый список"
					Description: При включении все предметы будут добавлены в белый список по умолчанию. {$Mods.OreExcavator.Config.Common.Performance.EnableImproves}!
				}

				ItemWhitelist: {
					Label: "[i/p0:38]{$Mods.OreExcavator.Config.Common.IconBuffer} Белый список предметов"
					Description:
						'''
						Настройте этот список, чтобы вручную указать, какие предметы могут быть моментально заменены. 
						Префикс определяет мод, которому они принадлежат. Также подчиняется черному списку хоста. {$Mods.OreExcavator.Config.Common.Performance.NoImpact}.
						'''
				}

				ToggleExcavations: {
					Label: "[i/p0:513]{$Mods.OreExcavator.Config.Common.IconBuffer} Назначение клавиш для переключения режима добычи"
					Description: При включении нажатие клавиши переключит активное состояние начала добычи. {$Mods.OreExcavator.Config.Common.Performance.NoImpact}.
				}

				ReleaseCancelsExcavations: {
					Label: "[i/p0:166]{$Mods.OreExcavator.Config.Common.IconBuffer} Отменить мульти-добычу при отпускании клавиши"
					Description:
						'''
						При включении, отпускание клавиши управления добычей прекратит все операции добычи.	
						НЕ РЕКОМЕНДУЕТСЯ НА МНОГОПОЛЬЗОВАТЕЛЬСКИХ СЕРВЕРАХ!! {$Mods.OreExcavator.Config.Common.Performance.NoImpact}.
						'''
				}

				DoSpecials: {
					Label: "[i/p0:3230]{$Mods.OreExcavator.Config.Common.IconBuffer} Включить альтернативные функции"
					Description:
						'''
						При включении клиент разрешит особые действия без мульти-добычи. Отключите это, если не планируете использовать эти функции или привязываете добычу к Mouse1
						Умеренно влияет на производительность!
						'''
				}

				Keybind: {
					Label: "[g:25]{$Mods.OreExcavator.Config.Common.IconBuffer} Ищете свою клавишу"
					Description: Установите свою клавишу в разделе управления игрой. Здесь она просто для отображения и для тех, кто запутался.
				}

				ModVersions: {
					Label: "[g:3095]{$Mods.OreExcavator.Config.Common.IconBuffer} Совместимые версии мода"
					Description:
						'''
						Определяет последнюю сохраненную версию модифицированного контента. Используется модом внутренне для определения момента перепроверки наличия новых руд и т.д.
						 УДАЛИТЕ МЕНЯ ДЛЯ ПРИНУДИТЕЛЬНОЙ СИНХРОНИЗАЦИИ ДЛЯ ЛЮБЫХ/ВСЕХ МОДОВ
						'''
				}
			}
		}

